{
  "name": "xe_oneday_ng1_ts",
  "version": "1.0.0",
  "description": "XE OneDay NG1 + TS (Fake Umbraco JSON Server)",
  "private": true,
  "main": "index.html",
  "scripts": {
    "start": "concurrent --kill-others 'npm run build' 'npm run serve'",
    "build": "gulp watch --projdir App_Plugins/xeSample",
    "serve": "node  node_modules/json-server/bin  --watch ./fake/db.json  --id Id  --routes ./fake/routes.json  --static ./",
    "prod": "gulp prod --tsconfig App_Plugins/xeSample/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmorosinotto/XE_OneDay_NG1xTS.git"
  },
  "keywords": [
    "Typescript",
    "Angular",
    "Umbraco",
    "ngMetadata",
    "Browserify",
    "Gulp"
  ],
  "author": "Daniele Morosinotto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmorosinotto/XE_OneDay_NG1xTS/issues"
  },
  "homepage": "https://github.com/dmorosinotto/XE_OneDay_NG1xTS#readme",
  "peerDependencies": {
    "browserify": "^13.0.1",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "concurrently": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-param": "^1.0.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "insert-css": "^0.2.0",
    "json-server": "^0.8.9",
    "tsify": "^0.15.5",
    "typings": "^0.8.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "angular": "^1.5.5",
    "ng-metadata": "^1.11.1",
    "reflect-metadata": "^0.1.3"
  }
}
